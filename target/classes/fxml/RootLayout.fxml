<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.icrperusa.guideprint.viewcontroller.AppViewController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="Archivo">
            <items>
                  <MenuItem fx:id="stbclean" mnemonicParsing="false" text="Limpiar">
                     <graphic>
                        <ImageView fx:id="ivclear" fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true" />
                     </graphic></MenuItem>
                  <SeparatorMenuItem mnemonicParsing="false" />
              <MenuItem fx:id="stbclose" mnemonicParsing="false" onAction="#handCloseApp" text="Salir">
                     <graphic>
                        <ImageView fx:id="ivclose" fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true" />
                     </graphic></MenuItem>
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="AcciÃ³n">
            <items>
                  <MenuItem mnemonicParsing="false" text="Ultima Guia">
                     <graphic>
                        <ImageView fx:id="ivlast" fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true" />
                     </graphic></MenuItem>
              <MenuItem mnemonicParsing="false" text="Reporte">
                     <graphic>
                        <ImageView fx:id="ivreport" fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true" />
                     </graphic></MenuItem>
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Acerca" />
        </menus>
      </MenuBar>
   </top>
</BorderPane>
